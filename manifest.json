{
  "manifest_version": 3,
  "name": "Test Safari Extension",
  "version": "1.0",
  "host_permissions": ["<all_urls>"],
  "permissions": ["tabs", "activeTab", "scripting"],
  "background": {
    "service_worker": "dist/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/content.js"],
      "run_at": "document_end"
    }
  ]
}